"$schema": "http://json-schema.org/draft/2019-09/schema",
"title": "Product",
"type": "object",
"required": ["id", "name", "price"],
"properties": {
  "type": {
    "type": "number",
    "description": "Product identifier"
  },
  "name": {
    "type": "string",
    "description": "Name of the product"
  },
  "price": {
    "type": "number",
    "minimum": 0
  },
  "tags": {
    "type": "array",
    "items": {
      "type": "string"
    }
  },
  "stock": {
    "type": "object",
    "properties": {
      "warehouse": {
        "type": "number"
      },
      "retail": {
        "type": "number"
      }
    }
  }
}
}
